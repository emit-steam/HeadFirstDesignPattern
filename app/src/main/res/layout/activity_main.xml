<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <TextView
            android:id="@+id/strategy"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:text="Strategy Pattern"/>

    <TextView
            android:id="@+id/observer"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:text="Observer Pattern!"/>

    <TextView
            android:id="@+id/decorator"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:text="Decorator Pattern!"/>

    <TextView
            android:id="@+id/dagger_exam1"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:text="Dagger2 exam 1!"/>

    <TextView
            android:id="@+id/dagger_exam2"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:text="Dagger2 exam 2!"/>

    <TextView
            android:id="@+id/dagger_exam3"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:text="Dagger2 exam 3!"/>
</LinearLayout>